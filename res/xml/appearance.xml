    <SwipeView>
        <backgroundcolor>#049fe1</backgroundcolor>
        <selectedpagecolor>#145daf</selectedpagecolor>
        <unselectedpagecolor>#a2cee0</unselectedpagecolor>
        <gapwidth>45.0</gapwidth>
