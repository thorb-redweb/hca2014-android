        <boxheight>15.0</boxheight>
    </SwipeView>

    <Nyheder>
    </Nyheder>

    <Nyhedsside>
        <backbuttonimage>btnclose.png</backbuttonimage>
    </Nyhedsside>

    <PMList>
    </PMList>

    <PMessage>
        <backbuttonimage>btnclose.png</backbuttonimage>
    </PMessage>

    <PMGroupSettings>
        <backgroundcolor>#88FFFFFF</backgroundcolor>
    </PMGroupSettings>
